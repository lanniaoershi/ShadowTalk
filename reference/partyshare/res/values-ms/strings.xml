    <string name="party_share_strings_music_list_no_song_lead_txt">Tiada lagu dalam senarai main dikongsi. Cuba tambah beberapa lagu.</string>
    <string name="party_share_strings_music_list_no_photo_lead_txt">Tiada foto dalam album dikongsi. Cuba ambil beberapa foto atau muat naik daripada koleksi anda.</string>
