    <string name="party_share_strings_music_list_no_song_lead_txt">Ingen sanger i den delte spillelisten. Forsøk å legge til noen sanger.</string>
    <string name="party_share_strings_music_list_no_photo_lead_txt">Ingen bilder i det delte albumet. Forsøk å ta noen bilder eller laste opp fra samlingen din.</string>
