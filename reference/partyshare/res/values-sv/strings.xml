    <string name="party_share_strings_music_list_no_song_lead_txt">Den delade spellistan innehåller inga låtar. Lägg till några låtar.</string>
    <string name="party_share_strings_music_list_no_photo_lead_txt">Det delade albumet innehåller inga foton. Ta några foton eller överför från din samling.</string>
